truncate table ods_sjhpt_t_vac_basic_children_totals_zl;
truncate table ods_sjhpt_t_vac_basic_children_totals_xg;
from(
select a.*,nvl(b.child_id,'1') child_ids from (select *,ROW_NUMBER()over(PARTITION BY child_id ORDER BY update_time) rn 
from ods_sjhpt_t_vac_basic_children_new_ymxg where dt=(select max(dt) from ods_sjhpt_t_vac_basic_children_new_ymxg )) a
left join ods_sjhpt_t_vac_basic_children_totals b
on nvl(a.child_id,'1')= nvl(b.child_id,'1') where a.rn=1)
insert into ods_sjhpt_t_vac_basic_children_totals_zl
select
children_number
,inoculation_cardnumber
,children_name
,gender_code
,gender_name
,idcard_num
,class_of_registered_residence
,birth_certification
,birthdate
,birth_hospital
,hospital_level
,birth_weight
