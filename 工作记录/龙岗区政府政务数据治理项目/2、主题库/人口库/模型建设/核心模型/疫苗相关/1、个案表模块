--drop table if exists ods_sjhpt_t_vac_basic_children_totals_temp;
truncate table ods_sjhpt_t_vac_basic_children_totals_temp;
insert into ods_sjhpt_t_vac_basic_children_totals_temp 
select * from ods_sjhpt_t_vac_basic_children_totals_zl
union ALL
select aa.* from ods_sjhpt_t_vac_basic_children_totals  aa
left join ods_sjhpt_t_vac_basic_children_totals_xg bb
on nvl(aa.child_id,'1')=  nvl(bb.child_id,'1')
where bb.children_number is NULL
UNION ALL
select * from ods_sjhpt_t_vac_basic_children_totals_xg;

truncate table ods_sjhpt_t_vac_basic_children_totals;
insert into ods_sjhpt_t_vac_basic_children_totals select * from ods_sjhpt_t_vac_basic_children_totals_temp;

insert into ods_sjhpt_t_vac_basic_children_total_ls 
select aa.* from ods_sjhpt_t_vac_basic_children_totals  aa
inner join ods_sjhpt_t_vac_basic_children_totals_xg bb
on nvl(aa.child_id,'1')=  nvl(bb.child_id,'1');