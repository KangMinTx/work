--增量运行后制作与太极平台一样的增量表dm_rkk_syrkkb_temp
drop table if exists dm_rkk_syrkkb_temp_1;
create  table dm_rkk_syrkkb_temp_1 as
select 
id,
 syrkjzdz       ,-- 实有人口居住地址
    fwmc        ,--房屋名称
    zjlx        ,--证件类型
    zjhm        ,--证件号码
    bm          ,--别名
    xm          ,--姓名
    gj          ,--国籍
    gatbz       ,-- 港澳台标志
    csny        ,--出生年月
    xb          ,--性别
    mz          ,--民族
    jg          ,--籍贯
    zjxy        ,--宗教信仰
    lsrq        ,--来深日期
    hyzk        ,--婚姻状况
    chrq        ,--初婚日期
    swrq        ,--死亡日期
    jkzk        ,--健康状况
    byyx        ,--毕业院校
    hy          ,--行业
    zhy         ,-- 职业
    zc          ,--职称
    ysr         ,-- 月收入
    sg          ,--身高
    zy          ,--专业
    `by`          ,--兵役
    xx          ,--血型
    xw          ,--学位
    yhzgx       ,-- 与户主关系
    hjlx        ,--户籍类型
    yhj         ,-- 原户籍
    hklx        ,--户口类型
    hkxz        ,--户口性质
    sfds        ,--是否独生
    whcd        ,--文化程度
    zzmm        ,--政治面貌
    hkszd       ,-- 户口所在地
    gllb        ,--管理类别
    xzqhid                            ,--  行政区划id
    sftj        ,--是否统计
    gxbz        ,--更新标志
    zjhm2       ,-- 证件号码2
    xgljgid                           ,--   新管理机构id
    lkyy        ,--离开原因
    ckqk        ,--持卡情况
    bydt        ,--避孕动态
    sydt        ,--生育动态
    yx          ,--邮箱
    sj          ,--手机
    zzdh        ,--住宅电话
    zslb        ,--住所类别
    gzdw        ,--工作单位
    dwdz        ,--单位地址
    bgdh        ,--办公电话
    blr         ,-- 办理人
    blsj        ,--办理时间
    xgz         ,-- 修改者
    xgsj        ,--修改时间
    cjsj        ,--创建时间
    cjz         ,-- 创建者
    bz          ,--备注
    djr         ,-- 登记人
    djsj        ,--登记时间
    crsj        ,--插入时间
    yxx         ,-- 有效性
    yxrq        ,--有效日期
    jdbm        ,--街道编码
    jdmc        ,--街道名称
    sqbm        ,--社区编码
    sqmc        ,--社区名称
    wgbm        ,--网格编码
    wgmc        ,--网格名称
    house_code                        ,--      房屋编码
    building_code                     ,--         楼栋编码
    lxdh        ,--联系电话
    tzr         ,-- 同住人
    lyb         ,-- 来源表
    lyjg        ,--来源机构
    yxsj        ,--   运行时间
    byzd1       ,-- 备用字段1
    byzd2       ,-- 备用字段2
    byzd3       ,-- 备用字段3
    byzd4       ,-- 备用字段4
    byzd5       -- 备用字段5
from dm_rkk_syrkkb_temp a where not exists(
select 1 from dm_rkk_rkb_zl b where a.id=b.id
)
union ALL
(
select 
  id,
 syrkjzdz       ,-- 实有人口居住地址
    fwmc        ,--房屋名称
    zjlx        ,--证件类型
    zjhm        ,--证件号码
    bm          ,--别名
    xm          ,--姓名
    gj          ,--国籍
    gatbz       ,-- 港澳台标志
    csny        ,--出生年月
    xb          ,--性别
    mz          ,--民族
    jg          ,--籍贯
