    yhzgx       ,-- 与户主关系
    hjlx        ,--户籍类型
    yhj         ,-- 原户籍
    hklx        ,--户口类型
    hkxz        ,--户口性质
    sfds        ,--是否独生
    whcd        ,--文化程度
    zzmm        ,--政治面貌
    hkszd       ,-- 户口所在地
    gllb        ,--管理类别
    xzqhid                            ,--  行政区划id
    sftj        ,--是否统计
    gxbz        ,--更新标志
