        dt=(SELECT  MAX(dt) FROM std_qfzggj_aic_base) and
      exists(     select 1 from std_qfzggj_aic_jgbase b where b.dt=(select max(dt) from std_qfzggj_aic_jgbase) and a.entid=b.entid and b.jgorgcode='4500'
											)   
	) t2
