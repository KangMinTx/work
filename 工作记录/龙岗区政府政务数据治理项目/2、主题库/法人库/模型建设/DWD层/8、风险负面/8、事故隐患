	,sgdj		--事故等级
	,sglxdm		--事故类型代码
	,sglx		--事故类型
	,sgyy		--事故原因
	,qyw		--起因物
	,zhw		--致害物
	,baqxw		--不安全行为
	,baqzt		--不安全状态
	,sggk		--事故概况
	,xgsj		--修改时间
	,xxlylx		--信息来源类型
	,lyb		--来源表
	,lyjg		--来源机构
	,yxsj		--运行时间

from 
(
		select
				key			--统一社会信用代码+处罚文书号+事故发生时间
