insert overwrite table dwd_sgxx partition (dt='${DayBeforeStartTime}')


select
	key			--统一社会信用代码+处罚文书号+事故发生时间
	,ztsfdm		--主体身份代码
	,tyshxydm	--统一社会信用代码
	,zh	gsmc		--法人名称/字号
	,xzcfjdwsh	--行政处罚决定书文号
	,sgfsdd		--事故发生地点
	,sgfssj		--事故发生时间
	,sw			--死亡（含失踪或下落不明）
	,zsrs		--重伤人数
	,jxgyzdrs	--急性工业中毒人数
	,qsrs		--轻伤人数
	,swrshj		--伤亡人数合计
	,bqddsw		--不确定的死亡（含失踪或下落不明）
	,bqddzsrs	--不确定的重伤人数
	,bqddjxgyzd	--不确定的急性工业中毒
	,bqddqssrs	--不确定的轻伤伤人数
	,bqddswrshj	--不确定的伤亡人数合计
