,A.YM_SCCJMC=B.YM_SCCJMC                                                           
,A.JDBM=B.JDBM                                                            
,A.JDMC=B.JDMC                                                           
,A.SQBM=B.SQBM                                                           
,A.SQMC=B.SQMC                                                            
,A.WGBM=B.WGBM                                                            
,A.WGMC=B.WGMC                                                           
,A.SYRKJZDZ=B.SYRKJZDZ                                                           
,A.SYRKLXDH=B.SYRKLXDH                                                            
,A.SYRKBS=B.SYRKBS                                                            
,A.YXSJ=B.YXSJ                                                           
,A.ZZC=B.ZZC                                  
,A.SFYWC=B.SFYWC                                                            
,A.BYZD2=B.BYZD2                                                          
,A.BYZD3=B.BYZD3                                                            
,A.BYZD4=B.BYZD4                                                           
,A.BYZD5=B.BYZD5                                                          
,A.BYZD6=B.BYZD6                                                            
,A.BYZD7=B.BYZD7                                                           
,A.BYZD8=B.BYZD8                                                          
