MERGE INTO  LQGSM.DM_RKK_YMFK_YMB A
USING (SELECT
SZZBM                                                                                        
,MYKH                                                            
,SZZXM                                                            
,XBDM                                                            
,XBMC                                                            
,SFZ                                                            
,NL                                                            
,LXDH                                                            
,LXSJ                                                            
,XZZ                                                            
,YJRQ                                                            
,YMJZRQ                                                            
,YMJZDD                                                            
,MYLXDM                                                            
,MYLXMC                                                            
